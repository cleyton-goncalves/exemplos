{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::bkt-appl-uat",
                "arn:aws:s3:::bkt-appl-uat/*"
            ],
            "_comment": "Permite ações de Get, Put e List no bucket S3 bkt-appl-aut."
        }
    ]
}
